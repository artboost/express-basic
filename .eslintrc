{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "max-len": 0,
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "ignore"
      }
    ],
    "no-use-before-define": "warn",
    "linebreak-style": 0,
    "no-console": 0,
    "prefer-promise-reject-errors": 0,
    "camelcase": 0
  }
}
